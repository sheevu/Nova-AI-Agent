{
  "buildCommand": "npm install && npm install --prefix backend",
  "outputDirectory": ".",
  "installCommand": "npm install && npm install --prefix backend",
  "env": {
    "OPENAI_API_KEY": "@OPENAI_API_KEY"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
